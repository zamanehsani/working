          "_name": "ampCountdown2",
          "_customHTML": "<section class=\"ampCountdown2\" group=\"Countdown\" plugins=\"amp-main,amp-date-countdown,amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n    <header>Color</header>\n    <input type=\"color\" title=\"Counter Text\" name=\"counterText\" value=\"#ffffff\" selected>\n    <input type=\"color\" title=\"Card\" name=\"cardColor\" value=\"#39305d\" selected>\n    <input type=\"range\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" title=\"Opacity\" value=\"1\">\n    <input type=\"color\" title=\"Background\" name=\"bgCounter\" value=\"#39305d\" selected>\n    <input type=\"range\" name=\"cardBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" title=\"Opacity\" value=\"1\">\n    <header>Background</header>\n    <input type=\"checkbox\" title=\"Inner Padding\" name=\"innerPadding\">\n    <fieldset type=\"background\" name=\"bg\">\n        <input type=\"image\" title=\"Background Image\" value=\"../_images/background4.jpg\">\n        <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n    </fieldset>\n    <select title=\"Language\" name=\"lang\">\n        <option value=\"en\" selected>English</option>\n        <option value=\"ru\">Russian</option>\n    </select>\n    <input type=\"text\" name=\"zone\" title=\"Time Zone (+01:00)\" value=\"+01:00\" required>\n    <input type=\"text\" name=\"date\" title=\"End Date (2020-01-21)\" value=\"2021-01-01\" required>\n    <input type=\"text\" name=\"time\" title=\"End Time (03:14:08)\" value=\"03:14:08\" required>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\" checked>\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"bg.type!='color' && overlay\">\n    <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' && overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"amp-countdown-wrap\">\n            <amp-date-countdown class=\"amp-countdown mbr-row mbr-column\" end-date=\"{{date}}T{{time}}.0{{zone}}\" locale=\"{{lang}}\" layout=\"flex-item\">\n                <template type=\"amp-mustache\">\n                <div class=\"mbr-col-lg-3 mbr-col-md-3 card align-center\">\n                    <div class=\"item-wrap mbr-column\">\n                        <span class=\"count-num mbr-bold display-1\">{{d}}</span>\n                        <span class=\"count-text display-7\">{{days}}</span>\n                    </div>\n                </div>\n                <div class=\"mbr-col-lg-3 mbr-col-md-3 card align-center\">\n                    <div class=\"item-wrap mbr-column\">\n                        <span class=\"count-num mbr-bold display-1\">{{h}}</span>\n                        <span class=\"count-text display-7\">{{hours}}</span>\n                    </div>\n                </div>\n                <div class=\"mbr-col-lg-3 mbr-col-md-3 card align-center\">\n                    <div class=\"item-wrap mbr-column\">\n                        <span class=\"count-num mbr-bold display-1\">{{m}}</span>\n                        <span class=\"count-text display-7\">{{minutes}}</span>\n                    </div>\n                </div>\n                <div class=\"mbr-col-lg-3 mbr-col-md-3 card align-center\">\n                    <div class=\"item-wrap mbr-column\">\n                        <span class=\"count-num mbr-bold display-1\">{{s}}</span>\n                        <span class=\"count-text display-7\">{{seconds}}</span>\n                    </div>\n                </div>\n                </template>\n            </amp-date-countdown>\n        </div>\n    </div>\n</section>",
